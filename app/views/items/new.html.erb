<h1>new item</h1>

<%= form_for @item do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :category %>
    <%= f.text_field :category %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :image %>
    <%= f.text_field :image %>

    <%= f.submit%>

<% end %>