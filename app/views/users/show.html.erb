
<h1>Welcome <%= @user.name %></h1>

<% @user.items.each do |item|%>
    <div>
        <p><%= link_to item.name, item_path(item) %></p>
        
   </div>
<%end%>

<% @user.shops.each do |shop|%>
    <div>
        <p><%= link_to shop.item_id, shop_path %></p>
        
        
   </div>
<%end%>





<%= button_to "Edit my account", edit_user_path, method: :get %>
<br>
<%= button_to "See listings", items_path, method: :get %>
<br>
<%= button_to "Buy", new_user_shop_path(@user), method: :get%>

