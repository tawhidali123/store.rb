<h1>new user</h1>

<%= form_for @user do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label "style Preferance" %>
    <%= f.text_field :preference %>

    <%= f.label :gender %>
    <%= f.text_field :gender %>

    <%= f.submit%>

<% end %>